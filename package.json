{
  "name": "tubeyou-frontend",
  "version": "0.5.5",
  "private": true,
  "homepage": "https://wholesome-ghoul.github.io/",
  "scripts": {
    "start:dev": "NODE_ENV=development webpack server",
    "start": "NODE_ENV=production webpack server",
    "build": "cp -f public/* dist && NODE_ENV=production webpack",
    "deploy": "yarn build && gh-pages -d dist",
    "test:cypress": "yarn cypress run-ct",
    "test:cypress:open": "yarn cypress open-ct",
    "prepare": "husky install",
    "push-tags": "git push --follow-tags",
    "release": "standard-version && yarn push-tags",
    "release:minor": "standard-version --release-as minor && yarn push-tags",
    "release:patch": "standard-version --release-as patch && yarn push-tags",
    "release:major": "standard-version --release-as major && yarn push-tags"
  },
  "dependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@svgr/cli": "^6.2.1",
    "@svgr/plugin-jsx": "^6.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.24",
    "axios": "^0.26.0",
    "classnames": "^2.3.1",
    "cypress": "^9.5.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "gh-pages": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "sass": "^1.49.9",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@cypress/react": "^5.12.4",
    "@cypress/webpack-dev-server": "^1.8.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass-loader": "^13.0.2",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "@tubeu/components": "^1.0.5",
    "@tubeu/configs-webpack": "^0.1.17",
    "@tubeu/configs-tsconfig": "^0.0.6"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "proxy": "http://localhost:4000"
}
